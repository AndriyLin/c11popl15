C11 A6_reorder
{ x = 0; y = 0; }

P0                                                | P1                                                 ;
*y = 1                                            | fence(memory_order_acquire)                        ;
atomic_store_explicit(x, 1, memory_order_release) | r0 = atomic_load_explicit(x, memory_order_relaxed) ;
                                                  | beq r0, 0, END1                                    ;
                                                  | r1 = atomic_load_explicit(y, memory_order_relaxed) ;
                                                  | END1:                                              ;

exists (1=1)